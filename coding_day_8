class Solution(object):
    def strStr(self, haystack, needle):
        """
        :type haystack: str
        :type needle: str
        :rtype: int
        """
        a= len(haystack)
        b= len(needle)
        for i in range(a+b-1):
            if haystack[i:i+b]== needle:
                return i
            else:
                return -1
